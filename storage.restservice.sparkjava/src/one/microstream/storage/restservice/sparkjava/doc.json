{
    "handler": {
        "one.microstream.storage.restservice.sparkjava.RouteGetRoot": {
            "get": {
                "description": "get the user defined root",
                "parameter": {},
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "description": "root name"
                                    },
                                    "objectId": {
                                        "type": "integer",
                                        "format": "int64",
                                        "description": "the root object id"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "options": {
                "description": "get detailed informations"
            }
        },
        "one.microstream.storage.restservice.sparkjava.RouteGetObject": {
            "options": {
                "description": "get detailed informations"
            },
            "get": {
                "description": "get an object by its id",
                "parameters": [
                    {
                        "name": "objectId",
                        "in": "path",
                        "description": "object id of the requested object",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
					{
                        "name": "valueLength",
                        "description": "limit size of returned value elements to this value",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "fixedOffset",
                        "description": "index of the first fix sized element to fetch",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "fixedLength",
                        "description": "number of fix sized elements to be fetched",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
					{
                        "name": "variableOffset",
                        "description": "index of the first element in variable sized collections to be fetched",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "variableLength",
                        "description": "number of elements to be fetched from variable sized collections",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        }
                    },
                    {
                        "name": "format",
                        "description": "requested response format",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "json"
                            ]
                        }
                    },
                    {
                        "name": "references",
                        "description": "resolve top level references and return them with this request",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "type": "object",
                                "description": "one.microstream.storage.restadapter.ViewerRootDescription as Json"
                            }
                        }
                    }
                }
            }
        },
